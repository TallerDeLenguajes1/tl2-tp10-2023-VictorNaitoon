@{
    ViewData["Title"] = "Home Page";
}

@model List<Usuario>

<h2>Listado de Usuarios</h2>

<table class="table table-dark">
    <thead>
        <tr>
            <th hidden>ID</th>
            <th>Nombre</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <td hidden>@usuario.Id</td>
                <td>@usuario.NombreDeUsuario</td>
            </tr>
        }
    </tbody>
</table>